%p#notice= notice
%h1 Site Urls
%table
  %thead
    %tr
      %th Url
      %th Date
      %th{colspan: "3"}
  %tbody
    - @site_urls.each do |site_url|
      %tr
        %td= site_url.url
        %td= site_url.date
        %td= link_to 'Show', site_url
        %td= link_to 'Edit', edit_site_url_path(site_url)
        %td= link_to 'Destroy', site_url, method: :delete, data: { confirm: 'Are you sure?' }
%br
= link_to 'New Site Url', new_site_url_path
